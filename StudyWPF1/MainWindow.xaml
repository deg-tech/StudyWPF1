<Window x:Class="StudyWPF1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudyWPF1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle Name="JournalScreen" Style="{StaticResource JournalScreenStyle}"/>
        <Label Name="JournalName" Style="{StaticResource JournalNameStyle}">
            【売上】現金で売り上げた
        </Label>
        <Label Name="JournateDate" Style="{StaticResource JournalDateStyle}">
            令和3年5月23日(日)
        </Label>
        <Calendar Name="JournalCalenadr" Style="{StaticResource JournalCalendarStyle}"/>
        <Label Name="JournalMoneyLabel" Style="{StaticResource JournalMoneyLabelStyle}">
            金額
        </Label>
        <TextBox Name="JournalMoney" Style="{StaticResource JounalMoneryStyle}"/>
        <Label Name="JournalRemarksLabel" Style="{StaticResource JournalRemarksLabelStyle}">
            摘要
        </Label>
        <TextBox Name="JournalRemarks" Style="{StaticResource JournalRemarksStyle}">
            １２３４５６７８９０１２３４５６７８９０１２３４５６７８９０２１３４５６
        </TextBox>
    </Grid>
</Window>
